{
  "name": "MarIA - Gestor de Escalaciones",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "escalation-update",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook-escalation",
      "name": "Webhook Actualizar Escalacion",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [250, 300],
      "webhookId": "escalation-update"
    },
    {
      "parameters": {
        "jsCode": "// Parsear datos de la escalacion\nconst body = $input.first().json;\n\nconst escalationId = body.escalation_id;\nconst action = body.action; // 'resolve', 'dismiss', 'assign', 'respond'\nconst humanResponse = body.human_response || null;\nconst resolutionNotes = body.resolution_notes || null;\nconst assignedTo = body.assigned_to || null;\n\nif (!escalationId || !action) {\n  throw new Error('escalation_id y action son requeridos');\n}\n\nreturn {\n  json: {\n    escalationId,\n    action,\n    humanResponse,\n    resolutionNotes,\n    assignedTo\n  }\n};"
      },
      "id": "parse-escalation-request",
      "name": "Parsear Request",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [470, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "action-resolve",
              "leftValue": "={{ $json.action }}",
              "rightValue": "resolve",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "switch-action",
      "name": "Tipo de Accion",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 3,
      "position": [690, 300]
    },
    {
      "parameters": {
        "method": "PATCH",
        "url": "={{ $env.SUPABASE_URL }}/rest/v1/escalations",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "apikey",
              "value": "={{ $env.SUPABASE_ANON_KEY }}"
            },
            {
              "name": "Prefer",
              "value": "return=representation"
            }
          ]
        },
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "id",
              "value": "=eq.{{ $json.escalationId }}"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"status\": \"resolved\",\n  \"human_response\": {{ $json.humanResponse ? '\"' + $json.humanResponse + '\"' : 'null' }},\n  \"resolution_notes\": {{ $json.resolutionNotes ? '\"' + $json.resolutionNotes + '\"' : 'null' }},\n  \"resolved_at\": \"{{ new Date().toISOString() }}\"\n}",
        "options": {}
      },
      "id": "resolve-escalation",
      "name": "Resolver Escalacion",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [910, 140],
      "credentials": {
        "httpHeaderAuth": {
          "id": "supabase-auth",
          "name": "Supabase Auth"
        }
      }
    },
    {
      "parameters": {
        "method": "PATCH",
        "url": "={{ $env.SUPABASE_URL }}/rest/v1/escalations",
        "authentication": "genericCredentialType",
        "genericAuthType": "httpHeaderAuth",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "apikey",
              "value": "={{ $env.SUPABASE_ANON_KEY }}"
            },
            {
              "name": "Prefer",
              "value": "return=representation"
            }
          ]
        },
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "id",
              "value": "=eq.{{ $json.escalationId }}"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"status\": \"dismissed\",\n  \"resolution_notes\": {{ $json.resolutionNotes ? '\"' + $json.resolutionNotes + '\"' : '\"Caso descartado\"' }},\n  \"resolved_at\": \"{{ new Date().toISOString() }}\"\n}",
        "options": {}
      },
      "id": "dismiss-escalation",
      "name": "Descartar Escalacion",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [910, 300],
      "credentials": {
        "httpHeaderAuth": {
          "id": "supabase-auth",
          "name": "Supabase Auth"
        }
      }
    },
    {
      "parameters": {
        "method": "PATCH",
        "url": "={{ $env.SUPABASE_URL }}/rest/v1/escalations",
