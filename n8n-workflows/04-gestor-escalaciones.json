            },
            {
              "name": "Prefer",
              "value": "return=representation"
            }
          ]
        },
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            {
              "name": "id",
              "value": "=eq.{{ $json.escalationId }}"
            }
          ]
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"status\": \"in_progress\",\n  \"assigned_to\": \"{{ $json.assignedTo }}\"\n}",
        "options": {}
      },
      "id": "assign-escalation",
      "name": "Asignar Escalacion",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [910, 460],
      "credentials": {
        "httpHeaderAuth": {
          "id": "supabase-auth",
          "name": "Supabase Auth"
        }
      }
    },
    {
      "parameters": {
        "mode": "chooseBranch",
        "output": "all"
      },
      "id": "merge-actions",
      "name": "Unir Acciones",
      "type": "n8n-nodes-base.merge",
      "typeVersion": 3,
      "position": [1130, 300]
    },
    {
      "parameters": {
        "jsCode": "// Preparar respuesta\nconst result = $input.first().json;\nconst updatedEscalation = Array.isArray(result) ? result[0] : result;\n\nreturn {\n  json: {\n    success: true,\n    message: 'Escalacion actualizada correctamente',\n    escalation: updatedEscalation\n  }\n};"
      },
      "id": "prepare-response",
      "name": "Preparar Respuesta",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1350, 300]
    },
    {
